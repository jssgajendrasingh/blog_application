<h1 align="center">User Registation Form</h1>
<%= form_for( :user,url: admin_users_path) do |f|%>
	<p>
	  <%= f.label :firstname %>
	  <%= f.text_field :firstname %>
	</p>
	<p>
	  <%= f.label :lastname %>
	  <%= f.text_field :lastname %>
	</p>
  <p>
	  <%= f.label :about %>
	  <%= f.text_field :about %>
	</p>
	<p>
	  <%= f.label :email %>
	  <%= f.text_field :email %>
	</p>
	<p>
		<%= f.label :password %>
		<%= f.text_field :password %>
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>